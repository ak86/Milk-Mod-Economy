							</description>
							<image path="Fomod\Screens\MPNO.jpg"/>
							<conditionFlags>
								<flag name="MilkumpNone">None</flag>
							</conditionFlags>
							<files>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>				
						</plugin>
						<plugin name="Basic">
							<description>
								<![CDATA[Basic placement of milkpumps, not bound in towns, bound in orc strongholds]]>
							</description>
							<image path="Fomod\Screens\MPBasic.jpg"/>
							<conditionFlags>
								<flag name="MilkumpBasic">Basic</flag>
							</conditionFlags>
							<files>
								<folder source="02_Extra\MilkPumps Basic" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>				
						<plugin name="BasicNB">
							<description>
								<![CDATA[Basic placement of milkpumps, all pumps are not bound]]>
							</description>
							<image path="Fomod\Screens\MPBasic.jpg"/>
							<conditionFlags>
								<flag name="MilkumpBasicNB">Basic not bound</flag>
							</conditionFlags>
							<files>
								<folder source="02_Extra\MilkPumps Basic NB" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>				
						<plugin name="BasicB">
							<description>
								<![CDATA[Basic placement of milkpumps, all pumps are bound]]>
							</description>
							<image path="Fomod\Screens\MPBasic.jpg"/>
							<conditionFlags>
								<flag name="MilkumpBasicB">Basic bound</flag>
							</conditionFlags>
							<files>
								<folder source="02_Extra\MilkPumps Basic B" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>				
