						<plugin name="Read me">
							<description>
								<![CDATA[Installing plugin scripts for optional mods. Installing scripts without mod may lead to mod conflicts and mods not working.]]>
							</description>
							<conditionFlags>
								<flag name="Read me"></flag>
							</conditionFlags>
							<typeDescriptor>
								<type name="Required"/>
							</typeDescriptor>
						</plugin>
					</plugins>
				</group>
				<group name="Support plugins" type="SelectAny">
					<plugins order="Explicit">			
						<plugin name="SL Aroused (Redux)">
							<description>
							</description>
							<conditionFlags>
								<flag name="SLA">On</flag>
