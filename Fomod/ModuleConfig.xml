										<pattern>
											<dependencies>
												<fileDependency file="SexLabProcreation.esp" state="Active"/>
											</dependencies>
											<type name="Recommended"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
						</plugin>
					</plugins>   
