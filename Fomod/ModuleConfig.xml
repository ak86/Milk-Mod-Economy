							<description>
								<![CDATA[All inclusive 5* milking barns by EinarrTheRed, not bound in towns, bound in orc strongholds, orc milk caves]]>
							</description>
							<image path="Fomod\Screens\MPFancy.jpg"/>
							<conditionFlags>
								<flag name="MilkumpFancy">Fancy</flag>
							</conditionFlags>
							<files>
								<folder source="02_Extra\MilkPumps Fancy" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>				
						</plugin>				

					</plugins>   
