								<type name="Optional"/>
							</typeDescriptor>
						</plugin>
						<plugin name="Classic - Colored">
							<description>
								<![CDATA[RGB by Z0mBieP00Nani, White Background]]>
							</description>
							<image path="Fomod\Screens\Logo\Classic - Colored\MilkLogo.jpg"/>
