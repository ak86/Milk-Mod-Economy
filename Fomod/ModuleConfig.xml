										<pattern>
											<dependencies>
												<fileDependency file="dcc-soulgem-oven-000.esm" state="Active"/>
											</dependencies>
											<type name="Recommended"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
						</plugin>
						<plugin name="SL Hentai Pregnancy">
							<description>
								<![CDATA[Pregnancy detection.]]>
							</description>
							<conditionFlags>
								<flag name="SLHP">On</flag>
							</conditionFlags>
							<typeDescriptor>
								<dependencyType>
									<defaultType name="Optional"/>
									<patterns>
										<pattern>
											<dependencies>
												<fileDependency file="HentaiPregnancy.esm" state="Active"/>
											</dependencies>
											<type name="Recommended"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
						</plugin>
						<plugin name="Fertility Mode 2.x">
							<description>
								<![CDATA[Pregnancy detection. DO NOT USE WITH Fertility Mode 1.x]]>
							</description>
							<conditionFlags>
								<flag name="FM">On</flag>
							</conditionFlags>
							<typeDescriptor>
								<dependencyType>
									<defaultType name="Optional"/>
									<patterns>
										<pattern>
											<dependencies>
												<fileDependency file="Fertility Mode.esp" state="Active"/>
											</dependencies>
											<type name="Recommended"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
						</plugin>
						<plugin name="SL Procreation">
							<description>
								<![CDATA[Pregnancy detection.]]>
							</description>
							<conditionFlags>
								<flag name="SLP">On</flag>
							</conditionFlags>
							<typeDescriptor>
								<dependencyType>
									<defaultType name="Optional"/>
