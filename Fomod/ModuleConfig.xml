							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>
						<plugin name="Colour Milk">
							<description>
								<![CDATA[RGB with white border by Lizard2010]]>
							</description>
							<image path="Fomod\Screens\Logo\ColourMilk\MilkLogo.jpg"/>
							<conditionFlags>
								<flag name="LogoColourMilk">Active</flag>
							</conditionFlags>
							<files>
								<folder source="04_Logo\ColourMilk" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>
					</plugins>   
				</group>
				<group name="Extra" type="SelectAny">
					<plugins order="Explicit">			
						<plugin name="Brown Panty">
							<description>
								<![CDATA[Replaces Milkcuirass black&white panies with brown version by Saeros]]>
							</description>
							<conditionFlags>
								<flag name="Brown Panty">Brown Panty</flag>
							</conditionFlags>
							<files>
								<folder source="02_Extra\Brown Panty" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>				
						<plugin name="Campfire addon">
							<description>
								<![CDATA[Campfire addon, allows crafting and placing milkpumps]]>
							</description>
							<image path="Fomod\Screens\CF.jpg"/>
							<conditionFlags>
								<flag name="Campfire">Campfire</flag>
							</conditionFlags>
							<files>
								<folder source="02_Extra\Campfire" destination=""/>
							</files>
							<typeDescriptor>
								<dependencyType>
									<defaultType name="Optional"/>
									<patterns>
										<pattern>
											<dependencies>
												<fileDependency file="Campfire.esm" state="Active"/>
											</dependencies>
											<type name="Recommended"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
						</plugin>				
					</plugins>   
				</group>
				<group name="Patches" type="SelectAny">
					<plugins order="Explicit">			
						<plugin name="Hearthfire">
							<description>
								<![CDATA[Plugin for Hearthfire DLC, adds ability to craft "Jug of Milk" from basic milk potions at Cooking Pot]]>
							</description>
							<conditionFlags>
								<flag name="HF_Patch">Active</flag>
							</conditionFlags>
							<files>
								<folder source="03_Patches\HF" destination=""/>
							</files>
							<typeDescriptor>
								<dependencyType>
									<defaultType name="Optional"/>
									<patterns>
										<pattern>
											<dependencies>
												<fileDependency file="HearthFires.esm" state="Active"/>
											</dependencies>
											<type name="Recommended"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
						</plugin>
						<plugin name="MME Sandbox">
							<description>
								<![CDATA[Enables npc sandboxing for MME milkpumps - any npc can use pumps]]>
							</description>
							<conditionFlags>
								<flag name="Sandbox_Patch">Active</flag>
							</conditionFlags>
							<files>
								<folder source="03_Patches\Sandbox" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>				
						</plugin>
						<plugin name="Milkpump Collision Fix">
							<description>
								<![CDATA[Milking machines collision fix by CherryHotaling - npcs wont stuck in milkpump model, but it may be hard to target milkpumps]]>
							</description>
							<conditionFlags>
								<flag name="MilkpumpCollisionFix">Active</flag>
							</conditionFlags>
							<files>
								<folder source="03_Patches\MilkpumpCollisionFix" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>
					</plugins>   
				</group>
				<group name="ZaZ Animation Pack Support" type="SelectExactlyOne">
					<plugins order="Explicit">			
						<plugin name="No Patch">
							<description>
								<![CDATA[if you dont have for ZaZ Animation Pack 6.0+ or dont want Zaz dwemer machines to function as milkpumps]]>
							</description>
							<conditionFlags>
								<flag name="ZaZ_Patch">Active</flag>
							</conditionFlags>
							<files>
							</files>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>
						<plugin name="No sandbox">
							<description>
								<![CDATA[Will make dwarven machines from ZaZ pack work as milkpumps, this might conflict with other mods that run scripts on dwarven machines, use at your own risk. No Sandbox - npc ai will ignore furniture]]>
							</description>
							<conditionFlags>
								<flag name="ZaZ_No_sandbox">Active</flag>
							</conditionFlags>
							<files>
								<folder source="03_Patches\ZaZ" destination=""/>
							</files>
							<typeDescriptor>
								<dependencyType>
									<defaultType name="Optional"/>
									<patterns>
										<pattern>
											<dependencies>
												<fileDependency file="ZaZAnimationPack.esm" state="Active"/>
											</dependencies>
											<type name="Optional"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
						</plugin>
						<plugin name="Sandbox">
							<description>
								<![CDATA[Will make dwarven machines from ZaZ pack work as milkpumps, this might conflict with other mods that run scripts on dwarven machines, use at your own risk. Sandbox - npc ai may use furniture on their own]]>
							</description>
							<conditionFlags>
								<flag name="ZaZ_Patch_Sandbox">Active</flag>
							</conditionFlags>
							<files>
								<folder source="03_Patches\ZaZ Sandbox" destination=""/>
							</files>
							<typeDescriptor>
								<dependencyType>
									<defaultType name="Optional"/>
									<patterns>
										<pattern>
											<dependencies>
												<fileDependency file="ZaZAnimationPack.esm" state="Active"/>
											</dependencies>
											<type name="Optional"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
