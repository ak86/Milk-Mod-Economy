											</dependencies>
											<type name="Recommended"/>
										</pattern>
									</patterns>
								</dependencyType>
							</typeDescriptor>
						</plugin>
						<plugin name="iNeed">
							<description>
								<![CDATA[Adds MME food and drinks to iNeed.]]>
							</description>
							<conditionFlags>
								<flag name="iNeed">On</flag>
